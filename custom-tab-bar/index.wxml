<!--custom-tab-bar/index.wxml-->
<t-tab-bar value="{{value}}" bindchange="onChange" theme="tag" split="{{false}}">
  <t-tab-bar-item
    wx:for="{{list}}"
    wx:key="value"
    value="{{item.value}}"
    icon="{{item.icon}}"
    ariaLabel="{{item.label}}"
  >
    {{item.label}}
  </t-tab-bar-item>
</t-tab-bar>